Firewall Policy
edit 0
        set name "IPSECcin_IPSECsac"
        set srcintf "IPSEC_CIN"
        set dstintf "IPSEC_SAC"
        set srcaddr "all"
        set dstaddr "all"
        set action accept
        set schedule "always"
        set service "ALL"
        set fsso disable
    next
